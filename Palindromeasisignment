package palindrome_assignment_haley_moses

    class assignment2 {

        // return true if string is a palindrome and false if its not
        
        static boolean Palindrome(String A)
        {
            int i = 0, j = A.length() -1;
            while (i < j) {
if (A.charAt(i)!=A.charAt(j))
    return false;
        i++;    
        j--;
            }
        }
    return true;
    public static void main(String[]args)
    {
        String A = "geeks";
        if (Palindrome(A))
            System.out.print("Yes");
        else 
            System.out.print("No");
        }
    }
